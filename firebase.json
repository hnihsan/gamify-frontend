{
  "hosting": {
    "public": "./dist",
    "headers": [{
      "source" : "**/*.@(js|html)",
      "headers" : [
        {
        "key" : "Cache-Control",
        "value" : "max-age=0"
        },
        {
          "key" : "Access-Control-Allow-Origin",
          "value" : "*"
        }
      ]
    }]
  }
}
